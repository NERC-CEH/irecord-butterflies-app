  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@capacitor/cli": "3.0.2",
    "@flumens/eslint-config": "0.4.0",
    "@flumens/fetch-onedrive-excel": "0.1.1",
    "@flumens/webpack-config": "2.2.0",
    "@types/leaflet": "^1.7.8",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-router": "5.1.16",
    "@types/react-router-dom": "5.3.0",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.6",
    "babel-plugin-module-resolver": "4.1.0",
    "cordova-res": "0.15.3",
    "dotenv": "10.0.0",
    "eslint": "7.30.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-import": "2.23.4",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-transform-stub": "2.0.0",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.1",
    "regenerator-runtime": "0.13.7",
    "tslib": "2.3.0",
    "typescript": "4.5.4",
    "vercel": "23.0.1",
    "webpack": "5.42.1",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "browserslist": [
    "Android >= 5.1",
    "iOS >= 10"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@apps": "./src/common/appsBitCollection",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "models": "./src/common/models",
            "Components": "./src/common/Components"
          }
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-transform-exponentiation-operator",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@flumens",
      "plugin:@typescript-eslint/recommended"
    ],
    "settings": {
      "import/resolver": {
        "babel-module": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      },
      "plugins": [
        "@typescript-eslint"
      ]
    },
    "rules": {
      "jsx-a11y/accessible-emoji": 0,
      "@typescript-eslint/no-var-requires": 0,
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "@typescript-eslint/no-empty-interface": "off",
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ],
      "react/prop-types": "off",
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.21"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "jest": {
    "globals": {},
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "((src/\\.|/*.)(spec))\\.js?$",
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "ionic-native": "jest-transform-stub",
      "react-photoswipe": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "components",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic-native|@ionic|@bit)"
    ]
  }
}
