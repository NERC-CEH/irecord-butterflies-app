{
  "name": "irecord-butterflies",
  "version": "2.4.0",
  "scripts": {
    "start": "webpack serve --progress --port 8000",
    "build": "NODE_ENV=production webpack",
    "build:production": "npm run clean && npm run build && npx cap sync && sed -i '' 's/natural_apptitude/natural-apptitude/g' ios/App/App/capacitor.config.json && npm run build:resources",
    "build:resources": "mkdir -p resources && cp -R other/designs/android resources && cp -R other/designs/*.png resources && npx cordova-res ios --skip-config --resources resources --copy && npx cordova-res android --skip-config --resources resources --copy",
    "build:probabilities": "cd src/common/data/species && node makeProbabilities.js",
    "deploy": "cp other/demo.html build/ && URL=$(vercel --prod -t $VERCEL_TOKEN --confirm ./build) && CLEAN_URL=$(echo $URL | sed -r \"s/https:\\/\\/(.*)$/\\1/g\") && DEMO_URL=$(echo $CLEAN_URL | sed -r \"s/(.*\\-)[a-z0-9]+\\-flumens(\\.vercel\\.app)$/\\1${BITRISE_BUILD_NUMBER}\\2/g\") && vercel alias set $CLEAN_URL $DEMO_URL -t $VERCEL_TOKEN && envman add --key DEMO_URL --value \"https://$DEMO_URL\"",
    "deploy:production": "bitrise ci",
    "clean": "rm -rf build resources",
    "test": "jest --silent --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch test",
    "test:eslint": "eslint src --quiet --ext .jsx --ext .ts --ext .tsx --ext .js && echo '\\033[37;42;1m PASS \\033[00m'",
    "fetch:data": "cd src/common/data/species && node make.js"
  },
  "dependencies": {
    "@capacitor-community/background-geolocation": "^1.2.2",
    "@capacitor/android": "^3.5.1",
    "@capacitor/app": "^1.1.1",
    "@capacitor/camera": "^1.3.1",
    "@capacitor/core": "^3.5.1",
    "@capacitor/filesystem": "^1.1.0",
    "@capacitor/geolocation": "^1.3.1",
    "@capacitor/haptics": "^1.1.4",
    "@capacitor/ios": "^3.5.1",
    "@capacitor/keyboard": "^1.2.2",
    "@capacitor/network": "^1.0.7",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "^1.0.8",
    "@flumens/bigu": "^0.4.0",
    "@flumens/ionic": "^1.2.0",
    "@indicia-js/core": "6.1.0",
    "@indicia-js/remote": "^6.1.4",
    "@ionic-native/core": "5.36.0",
    "@ionic-native/launch-review": "^5.29.0",
    "@ionic/core": "^6.1.9",
    "@ionic/react": "^6.1.9",
    "@ionic/react-router": "^6.1.9",
    "@mapbox/geojson-area": "^0.2.2",
    "@sentry/browser": "^6.19.7",
    "@sentry/integrations": "^6.19.7",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "cordova-sqlite-storage": "6.0.0",
    "deep-diff": "^1.0.2",
    "formik": "2.2.9",
    "i18next": "^21.8.9",
    "ionicons": "^6.0.2",
    "jwt-decode": "^3",
    "leaflet": "1.7.1",
    "leaflet-draw": "^1.0.4",
    "localforage-cordovasqlitedriver": "1.8.0",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "mobx-utils": ">=6",
    "mock-geolocation": "1.0.11",
    "proj4": "^2",
    "proj4leaflet": "^1",
    "prop-types": "15.8.1",
    "prop-types-exact": "1.2.0",
    "react": "^17.0.2",
    "react-autosuggest": "^10",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.17.1",
    "react-leaflet": "3.2.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.8.0",
    "swiper": "^8.2.2",
    "wicket": "^1.3.8",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@capacitor/cli": "^3.5.1",
    "@flumens/eslint-config": "^2.0.1",
    "@flumens/fetch-onedrive-excel": "^0.3.0",
    "@flumens/webpack-config": "^4.1.2",
    "@types/jest": "^27.5.1",
    "@types/leaflet": "^1.7.10",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "^28.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cordova-res": "^0.15.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "fast-csv": "^4.3.6",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-transform-stub": "2.0.0",
    "regenerator-runtime": "^0.13.9",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vercel": "^24.2.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "browserslist": [
    "Android >= 5.1",
    "iOS >= 10"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "babel": {
    "presets": [
      [
        "@babel/react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@flumens$": "./src/common/flumens",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "models": "./src/common/models",
            "Components": "./src/common/Components"
          }
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-transform-exponentiation-operator",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@flumens",
      "plugin:react/jsx-runtime"
    ],
    "rules": {
      "react/function-component-definition": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": 0,
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "babel-jest",
        {
          "presets": [
            "@babel/env"
          ]
        }
      ]
    },
    "testRegex": "src.*((\\.|/*.)(spec))\\.(t|j)s?$",
    "moduleNameMapper": {
      "^.+.(css|style|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "ionic-native": "jest-transform-stub",
      "swiper": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "components",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic|@stencil|ionicons)"
    ]
  }
}
